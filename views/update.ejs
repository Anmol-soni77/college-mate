                <div class="accordion-body">

                    <form class="needs-validation" action="/update/name" method="post" enctype="multipart/form-data" novalidate>

                        <div class="row g-3">
                          <div class="col-sm-6">
                            <label for="name" class="form-label">Name</label>
                            <input type="text" name="updateterm" class="form-control" id="name" placeholder="Enter Your name" value="" required>
                            <div class="invalid-feedback">
                              Valid name is required.
                            </div>
                          </div>

                          <button class="w-20 btn btn-primary btn-md" type="submit">Update</button>

                          </form>
                </div>
              </div>
            </div>

            <div class="accordion-item">
