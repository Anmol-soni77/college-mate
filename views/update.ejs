<%- include('./partials/navbar') %>

    <div class="container p-5 yoo">

      
      <div class="row p-3 mb-3 bg-info bg-opacity-10 border border-info border-start-0 rounded-end" style="background-color: aliceblue;">
        <div class="col-6 d-flex justify-content-center align-items-center">
            <h3 class="text-white">Update your profile </h3>
        </div>
