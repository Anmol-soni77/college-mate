<%- include('./partials/navbar') %>

<div class="container yoo my-5">
            
  <hr style="color:white;">
  <h3>Result</h3>
  <hr style="color:white;">
  
      <div class="row align-items-center justify-content-center">
          <% if(users != "") { %>
              <% for(const element of users){  %>
          <div class="col-6 col-sm-4 col-lg-3 card bg-dark text-light m-3" style="width: 18rem;">
              <img src="<%- element.image %>" class="card-img-top"  height="120px" alt="...">
              <div class="card-body">
                <h5 class="card-title"><%- element.username %></h5>
                <p class="card-text">Hey, It's <%- element.name %> , I am from  <%- element.college %> to know more about me check out my profile.</p>
              </div>
              <div class="card-body">
                  <a href="/student/<%- element.username %>"><button class="btn btn-outline-primary"> View profile </button> </a>
              </div>
            </div>
            <% } %>
      <% }else{ %>
        <h3>NO Users found</h3>
        <% } %>

      </div>
</div>