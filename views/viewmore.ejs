<%- include('./partials/navbar') %>

<div class="container yoo my-5">
            
  <hr style="color:white;">
  <h3>Result</h3>
  <hr style="color:white;">
  
      <div class="row align-items-center justify-content-center">
          <% if(users != "") { %>
              <% for(const element of users){  %>
          <div class="col-6 col-sm-4 col-lg-3 card bg-dark text-light m-3" style="width: 18rem;">
              <img src="../images/<%- element.image %>" class="card-img-top"  height="120px" alt="...">
              <div class="card-body">
                <h5 class="card-title"><%- element.username %></h5>
                <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
              </div>
              <ul class="list-group list-group-flush">
                <li class="list-group-item bg-dark text-light ">An item</li>
                <li class="list-group-item bg-dark text-light ">A second item</li>
                <li class="list-group-item bg-dark text-light ">A third item</li>
              </ul>
              <div class="card-body">
                  <a href="/student/<%- element.username %>">View</a>
              </div>
            </div>
            <% } %>
      <% }else{ %>
        <h3>NO Users found</h3>
        <% } %>

      </div>
</div>